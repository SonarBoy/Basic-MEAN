<% include ./partials/header.ejs %>
<body>

    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">

      <div class="carousel-inner">

        <div class="carousel-item active">
          <img class="d-block w-100" src="/images/Galaxy.jpg" alt="First slide">
        </div>
        <div class="carousel-item">
          <img class="d-block w-100" src="/images/carousel-2.jpg" alt="Second slide">
        </div>
        <div class="carousel-item">
          <img class="d-block w-100" src="/images/Milk.jpg" alt="Third slide">
        </div>
        
      </div>

      <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>

      <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>

    </div>    




    <div class = "container">
    <div class="row featurette">
        <div class="col-md-7">
          <h2 class="featurette-heading"><%= proj %> <span class="text-muted">Planets Example</span></h2>
          <p class="lead">This is a simple Mongo pull that describes the planets in the Solar System.</p>
          
          <% if (alertStyle == "Success") { %>
            <div class="alert alert-success" role="alert">
              <a href="" class="alert-link">...</a>
            </div>
          <% } else if(alertStyle == "Info") { %>
          <div class="alert alert-info" role="alert">
            <a href="#" class="alert-link">...</a>
          </div>
          <% } else if(alertStyle == "Warn") { %>
          <div class="projectLink" role="alert">
            <h2><a href="/Planets" class="alert-link">You can find the link here...</a></h2>
          </div>
          <% } else if(alertStyle == "Danger") { %>
          <div class="alert alert-danger" role="alert">
            <a href="#" class="alert-link">...</a>
          </div>
          <% } %>

        </div>
        <div class="col-md-5">
        <img width="500" height="500" src="/images/Planet-Flex.jpg" alt="Planets" class="img-thumbnail rounded lineItem">    
        </div>
      </div>

      <hr class="featurette-divider">

      <div class="row featurette">
        <div class="col-md-7 order-md-2">
          <h2 class="featurette-heading"><%= proj %> <span class="text-muted">Celestial Objects Example</span></h2>
          <p class="lead">This is a simple Mongo pull that describes Celestial Objects.</p>
        
          <% if (alertStyle == "Success") { %>
